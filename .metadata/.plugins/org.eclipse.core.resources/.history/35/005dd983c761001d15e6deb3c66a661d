class Student{

	String studentId;
	int age;
	
	void insertRecord(String studId, int newAge) {
		studentId = studId;
		age = newAge;
	}
	
	void displayRecord() {
		System.out.println(studentId + " " + age);
		
	}
}



public class MainApplication {

	public static void main(String[] args) {
		
		Student student = new Student();
		
		student.insertRecord("Student123", 10);
		student.displayRecord();
		
//		Student student = new Student();
//		student.studentId = "Jonathan";
//		student.age = 10;
//		System.out.println(student.studentId);
//		System.out.println(student.age);
		
		
	}
}
